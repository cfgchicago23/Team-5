// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id               Int    @id @default(autoincrement())
  email            String @unique
  phoneNumber      String
  firstname        String
  lastname         String
}

model Post {
  id               Int     @id @default(autoincrement())
  date             String
  caption          String  
}